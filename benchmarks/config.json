{
  "defaults": {
    "iterations": 100,
    "warmup": 5,
    "parallel_levels": [1, 2, 4, 8]
  },
  "llm": {
    "iterations": 10,
    "warmup": 3,
    "provider": "ollama",
    "model": "gpt-oss:120b-cloud",
    "model_priority": ["gpt-oss:120b-cloud", "gpt-oss:20b-cloud"],
    "base_url": "http://localhost:11434"
  },
  "workloads": {
    "parallel_research": {
      "description": "3-way parallel research with merge and synthesis",
      "parallel_ops": 3,
      "measures": ["parallelism_efficiency", "total_latency", "per_op_overhead"]
    },
    "scalability_curve": {
      "description": "Test parallelism at N = 1, 2, 4, 8 concurrent operations",
      "parallel_levels": [1, 2, 4, 8],
      "measures": ["speedup", "efficiency"]
    },
    "chain_fusion": {
      "description": "Demonstrate FuseReasoning compiler optimization",
      "chain_length": 5,
      "measures": ["llm_calls", "latency_reduction"]
    },
    "type_error": {
      "description": "Compare compile-time vs runtime error detection",
      "measures": ["time_to_error", "cost_wasted"]
    }
  },
  "output": {
    "format": "json",
    "directory": "results",
    "include_samples": true
  },
  "statistics": {
    "confidence_level": 0.95,
    "percentiles": [50, 95, 99]
  }
}
