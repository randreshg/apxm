// Scalability Workflow - 2-way parallel
// Tests parallelism efficiency with N=2 concurrent RSN operations.

agent ScalabilityN2 {
    @entry flow main() -> str {
        // 2 parallel RSN ops (T_1=2, T_inf=1, theoretical speedup=2x)
        ask("Provide a brief fact about science") -> a
        ask("Provide a brief fact about history") -> b
        merge(a, b) -> result
        print("N=2 Parallel Result")
        print(result)
        return result
    }
}
